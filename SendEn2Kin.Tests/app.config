<configuration>
	<appSettings>
		<add key="authToken" value="your_auth_token" />
		<add key="evernoteHost" value="sandbox.evernote.com" />
	</appSettings>

	<system.net>
		<mailSettings>
			<smtp from="your.sender.email@host.com">
				<network host="smtpserver" port="25" defaultCredentials="true" />
			</smtp>
		</mailSettings>
	</system.net>

	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>